{
  "expo": {
    "name": "InEvent",
    "slug": "InEvent",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/indexpage/icon.png",
    "splash": {
      "image": "./assets/indexpage/icon.png",
      "backgroundColor": "#ffffff",
      "resizeMode": "contain"
    },
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a07d4812-907c-4f2d-8c08-34af666b43a3"
      }
    },
    "android": {
      "package": "com.jonass7896.InEvent"
    }
  }
}
